{
    "welcomeFile": "/index.html",
    "routes": [
        {
            "source": "^/api(.*)$",
            "target": "$1",
            "destination": "api",
            "csrfProtection": false
        },
        {
            "source": "^/(.*)$",
            "target": "$1",
            "localDir": "./dist"
        }
    ]
}
